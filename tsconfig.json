{
  "compilerOptions": {
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "include": ["./src/*/.ts"]
}
